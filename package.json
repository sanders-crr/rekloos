{
  "name": "web-crawler-service",
  "version": "1.0.0",
  "description": "Web crawling service with Elasticsearch indexing",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "worker": "node src/worker.js",
    "test": "jest",
    "test:unit": "node scripts/test-runner.js unit",
    "test:integration": "node scripts/test-runner.js integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:unit": "node scripts/test-runner.js unit --coverage",
    "test:coverage:integration": "node scripts/test-runner.js integration --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bull": "^4.12.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "puppeteer": "^21.5.0",
    "winston": "^3.11.0",
    "robots-parser": "^3.0.1",
    "url-parse": "^1.5.10",
    "mime-types": "^2.1.35",
    "node-cron": "^3.0.3",
    "@elastic/elasticsearch": "^8.11.0",
    "express-rate-limit": "^7.1.5",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "eslint": "^8.54.0",
    "supertest": "^6.3.3",
    "@jest/globals": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}